{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "useUnknownInCatchVariables": false,
    "esModuleInterop": true,
    "declaration": false,
    "resolveJsonModule": true,
    "noEmit": true,
    "exactOptionalPropertyTypes": true
  }
}
